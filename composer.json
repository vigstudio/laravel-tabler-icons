{
    "name": "vigstudio/laravel-tabler-icons",
    "description": "The Tabler Icons Blade Component with Facade Support",
    "version": "2.0.0",
    "keywords": [
        "laravel-tabler-icons",
        "tabler-icons",
        "icons",
        "svg-icons",
        "blade-components",
        "facade",
        "laravel",
        "tailwind-css"
    ],
    "homepage": "https://github.com/vigstudio/laravel-tabler-icons",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "VigStudio",
            "email": "admin@nghiane.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/view": "^11.0|^12.0"
    },
    "require-dev": {
        "ext-zip": "*",
        "illuminate/filesystem": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "VigStudio\\TablerIcons\\": "src"
        }
    },
    "scripts": {
        "icons": "bin/icons",
        "example": "bin/example"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "VigStudio\\TablerIcons\\TablerIconsServiceProvider"
            ],
            "aliases": {
                "TablerIcon": "VigStudio\\TablerIcons\\Facades\\TablerIcon"
            }
        }
    }
}